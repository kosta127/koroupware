<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kosta.login.mapper.LoginMapper">
	<cache />
	
	
	<select id="checkMember" resultType="Member">
			select * from emp
	</select>
	
	<select id = "FindId_ListMember" resultType="Member">
		select emp_id, emp_no from emp 
	</select>
	
	<select id="FindPassword_ListMember" resultType="Member">
		select emp_password, emp_no, emp_id from emp
	</select>


</mapper>
